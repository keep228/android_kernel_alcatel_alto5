obj-$(CONFIG_SENSORS_AKM09911_MAG)		+= akm09911.o

ifdef CONFIG_APDS9930_ALSPRX
obj-$(CONFIG_APDS9930_ALSPRX)		+= apds993x.o
	EXTRA_CFLAGS += -DPSENSOR_FAC_CALIBRATION
	EXTRA_CFLAGS += -DPSENSOR_PHONE_CALIBRATION
	EXTRA_CFLAGS += -DALS_COMPENSATION
endif

#
# Makefile for Bosch sensor driver.
#
obj-y += bstclass.o

obj-$(CONFIG_SENSORS_BMA2X2)	+= bma2x2.o

ifeq ($(CONFIG_SENSORS_BMA2X2_ENABLE_INT1),y)
	EXTRA_CFLAGS += -DBMA2X2_ENABLE_INT1
endif

ifeq ($(CONFIG_SENSORS_BMA2X2_ENABLE_INT2),y)
	EXTRA_CFLAGS += -DBMA2X2_ENABLE_INT2
endif

