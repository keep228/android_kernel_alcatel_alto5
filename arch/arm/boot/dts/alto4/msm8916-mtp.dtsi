	};

	i2c@78ba000 { /* BLSP1 QUP6 */
		nfc@28 {
			compatible = "nxp,pn547";
			reg = <0x28>;
			qcom,irq-gpio = <&msm_gpio 21 0x00>;
			qcom,ven-gpio = <&msm_gpio 20 0x00>;
			qcom,dwl-gpio = <&msm_gpio 16 0x00>;
			qcom,clk-src = "BB_CLK2";
			qcom,clk-en-gpio = <&msm_gpio 0 0x00>;
			uim-supply = <&pm8916_l14>;
			interrupt-parent = <&msm_gpio>;
			interrupts = <21 0>; 
			qcom,clk-gpio = <&pm8916_gpios 2 0>; 
			clocks = <&clock_rpm clk_bb_clk2_pin>;
			clock-names = "ref_clk";
		};
	};

	i2c@78b6000 { /* BLSP1 QUP2 */
		akm@c {
			compatible = "ak,ak09911";
			reg = <0x0c>;
			pinctrl-names = "default","sleep";
			pinctrl-0 = <&akm_default>;
			pinctrl-1 = <&akm_sleep>;
			vdd-supply = <&pm8916_l17>;
			vio-supply = <&pm8916_l6>;
			akm,layout = <0x3>;
			akm,gpio_rstn = <&msm_gpio 8 0x0>;
			akm,auto-report;
		};
	    bosch@18 { /* Accelerometer sensor [Bug SW.Alto5_Global 724579 ] modify by xcb 2014-07-22 */
	        compatible = "bosch,bma2x2";
	        reg = <0x18>;
	        pinctrl-names = "bma_default","bma_sleep";
	        pinctrl-0 = <&bma2x2_int1_default &bma2x2_int2_default>;
	        pinctrl-1 = <&bma2x2_int1_sleep &bma2x2_int2_sleep>;
	        interrupt-parent = <&msm_gpio>;
	        interrupts = <115 0x2002>;
	        vdd-supply = <&pm8916_l6>;
	        vio-supply = <&pm8916_l6>;
	        bosch,init-interval = <200>;
	        bosch,place = <6>;
	        bosch,gpio-int1 = <&msm_gpio 115 0x2002>;
	        bosch,gpio-int2 = <&msm_gpio 114 0x2002>;
	    };
		mc3xxx@4c { /* [BUFFIX]-Add- by TCTNB.XQJ,PR-870540, 2014/12/16, add 2nd g sensors mc3xxxx*/
	        compatible = "mc3,mc3xxx";
	        reg = <0x4c>;
	        interrupt-parent = <&msm_gpio>;
	        interrupts = <115 0x2002>;
	        vdd-supply = <&pm8916_l6>;
			mc3xxx,delay = <60>;
	        mc3xxx,orientation = <4>; /* [BUFFIX]-Add- by TCTNB.XQJ,PR-901955, 2014/01/15,adjust right orientation*/
			mc3xxx,vpp = <&msm_gpio 114 0x0>;
	        mc3xxx,IRQ = <&msm_gpio 115 0x2002>;

	    };
		/*yongzhong.cheng,2014/07/24,PR 724584: add proximity and light sensor config begin*/
                tsl2772@39 {
                    compatible = "taos,tsl2772";
                        reg = <0x39>;
                        interrupt-parent = <&msm_gpio>;
                        interrupts = <113 0x2002>;
                        vdd-supply = <&pm8916_l17>;
                        vio-supply = <&pm8916_l6>;
                        tsl,irq-gpio = <&msm_gpio 113 0x2002>;
                        tsl,als-gate = <10>;    //
                        tsl,als-gain = <8>;     //
                       pinctrl-names = "default","sleep";/* [BUFFIX]-Add- by TCTNB.XQJ,PR-886227, 2015/2/2, init int gpio*/
                       pinctrl-0 = <&tsl2772x_default>;
                       pinctrl-1 = <&tsl2772x_sleep>;
			tsl,control-reg = <0x61>; // modify by ning.wei for pr743248 2014-07-18
			tsl,wait-reg = <0x00>;
			tsl,ps-thdh = <900>;
			tsl,ps-thdl = <750>;
			tsl,ps-ppcount = <12>; // add by ning.wei for pr743248 2014-07-18
		};
                /*yongzhong.cheng,2014/07/24,PR 724584: add proximity and light sensor config end*/
		
		avago@39 { /* Ambient light and proximity sensor */
			compatible = "avago,apds9900";
			reg = <0x39>;
			pinctrl-names = "default","sleep";
			pinctrl-0 = <&apds99xx_default>;
			pinctrl-1 = <&apds99xx_sleep>;
			interrupt-parent = <&msm_gpio>;
			interrupts = <113 0x2002>;
			vdd-supply = <&pm8916_l17>;
			vio-supply = <&pm8916_l6>;
			avago,irq-gpio = <&msm_gpio 113 0x2002>;
			avago,ps-threshold = <600>;
			avago,ps-hysteresis-threshold = <500>;
			avago,ps-pulse = <8>;
			avago,ps-pgain = <0>;
			avago,als-B = <223>;
			avago,als-C = <70>;
			avago,als-D = <142>;
			avago,ga-value = <48>;
		};

		akm@c {
			compatible = "ak,ak8963";
			reg = <0x0c>;
			pinctrl-names = "ak8963_default", "ak8963_sleep";
			pinctrl-0 = <&ak8963_default>;
			pinctrl-1 = <&ak8963_sleep>;
